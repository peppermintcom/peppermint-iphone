GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/APAddressBook" "${PODS_ROOT}/Headers/Public/EasyMailSender"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/APAddressBook" -isystem "${PODS_ROOT}/Headers/Public/EasyMailSender"
OTHER_LDFLAGS = $(inherited) -ObjC -l"APAddressBook" -l"EasyMailSender" -framework "AddressBook" -framework "MessageUI"
PODS_ROOT = ${SRCROOT}/Pods